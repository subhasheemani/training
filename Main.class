import java.util.Scanner;
import java.
class Sample{
    public static void main(String[] args) {
        ArrayList<Integer> as=new ArrayList<>();
        Scanner s=new Scanner(System.in);
        as.add(45);
        as.add(34);
        as.add(4);
        List<Integer> li=as.stream().map(x->Fact(x)).collect(Collectors.toList());

    }
    Public static int Fact(int i){
        int fact=1;
        for(int j;j<=i;j++){
            fact=fact*j;
        }
        return fact;
    }
}